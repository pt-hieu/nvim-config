{
  "Console Log": {
    "prefix": "log",
    "body": ["console.log($1)"],
    "description": "Console log"
  },
  "useState": {
    "prefix": "uc",
    "body": ["const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useState($2)"],
    "description": "React useState hook"
  },
  "useEffect": {
    "prefix": "ue",
    "body": [
      "useEffect(() => {",
      "\t$1",
      "}, [$2])"
    ],
    "description": "React useEffect hook"
  }
}
